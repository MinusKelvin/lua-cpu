local x = 0
local y = 0
local bgColor = 0
local fgColor = -1
local isBlinked = false
local blink = true
local closed = false
local cursorStyle = "underline"

local function write(b)
	if closed then
		error("cannot write to closed stream", 2)
	end
	if isBlinked then
		terminal.blinkCursor()
	end
	b = string.char(b)
	for i=1, 9 do
		screen.setPixel(x*6+1, 602-(y*10+i), bgColor)
	end
	for i=1, 6 do
		screen.setPixel(x*6+i, 602, bgColor)
	end
	if glyphs[b] then
		for i,row in ipairs(glyphs[b]) do
			for j,pix in ipairs(row) do
				screen.setPixel(x*6+j+1, 602-(y*10+i), (pix==1 and fgColor) or bgColor)
			end
		end
	else
		for i=1, 9 do
			for j=1, 5 do
				screen.setPixel(x*6+j+1, 602-(y*10+i), bgColor)
			end
		end
	end
	x = x + 1
	if x == 133 or b == "\n" then
		y = y + 1
		x = 0
	end
	terminal.blinkCursor()
	screen.update()
	return true
end

terminal = io.openOutputStream(write, function() closed = true end)
terminal.isTerminal = true

function terminal.clear()
	if closed then
		error("cannot write to closed stream", 2)
	end
	for i=1, 602 do
		for j=1, 800 do
			screen.setPixel(j, i, bgColor)
		end
	end
	terminal.setCursorPos(0,0)
	screen.update()
end

function terminal.setBgColor(c)
	if closed then
		error("cannot write to closed stream", 2)
	end
	bgColor = c
end

function terminal.setFgColor(c)
	if closed then
		error("cannot write to closed stream", 2)
	end
	fgColor = c
end

function terminal.blinkCursor()
	if closed then
		error("cannot write to closed stream", 2)
	end
	if not blink then
		return
	end
	isBlinked = not isBlinked
	local starty = 1
	local endy = 9
	local startx = 1
	local endx = 5
	if cursorStyle == "underline" then
		starty = 10
		endy = 10
	elseif cursorStyle == "line" then
		startx = 0
		endx = 0
	end
	for i=starty, endy do
		for j=startx, endx do
			screen.setPixel(x*6+j+1, 602-(y*10+i), bit.bnot(screen.getPixel(x*6+j+1, 602-(y*10+i))))
		end
	end
	screen.update()
end

function terminal.setCursorPos(_x, _y)
	if closed then
		error("cannot write to closed stream", 2)
	end
	if isBlinked then
		terminal.blinkCursor()
	end
	x = _x or x
	y = _y or y
	terminal.blinkCursor()
end

function terminal.moveCursor(_x, _y)
	if closed then
		error("cannot write to closed stream", 2)
	end
	if isBlinked then
		terminal.blinkCursor()
	end
	x = x + _x
	while x >= 133 do
		y = y + 1
		x = x - 133
	end
	y = y + _y
	terminal.blinkCursor()
end

function terminal.setBlinkEnabled(v)
	if closed then
		error("cannot write to closed stream", 2)
	end
	blink = v
end

function terminal.cursorStyle(style)
	if closed then
		error("cannot write to closed stream", 2)
	end
	local b = isBlinked
	if isBlinked then
		terminal.blinkCursor()
	end
	cursorStyle = (style == "default" and "block") or style or "block"
	if b then
		terminal.blinkCursor()
	end
end

function terminal.clearLine()
	if closed then
		error("cannot write to closed stream", 2)
	end
	for i=y*10, y*10+9 do
		for j=1, 800 do
			screen.setPixel(j, 602-i, bgColor)
		end
	end
	terminal.setCursorPos(0)
end

function terminal.getCursorPos()
	return x,y
end